@model List<Lab3.Objects.ChoreLaborer>

@{
    ViewData["Title"] = "Filtered Laborers";
}

<h2>Laborers List (Filtered by Age 3-10 and Difficulty <= 7)</h2>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Difficulty</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var laborer in Model)
        {
            <tr>
                <td>@laborer.Name</td>
                <td>@laborer.Age</td>
                <td>@laborer.Difficulty</td>
            </tr>
        }
    </tbody>
</table>
